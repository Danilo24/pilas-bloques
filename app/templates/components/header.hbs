<PaperToolbar class="header">
  <PaperToolbarTools>
    {{#paper-button disabled=false raised=false fab=false href=(href-to "index") bubbles=true}}<img class="pilas-bloques-logo" src='imagenes/logo.png'>{{/paper-button}}
    {{#paper-button disabled=false raised=false fab=false href=(href-to "libros.verLibro" 1) bubbles=true}}{{paper-icon "menu_book" class="menu-button-icon"}}<span class="menu-button-typography">Libro 1</span>{{/paper-button}}
    {{#paper-button disabled=false raised=false fab=false href=(href-to "libros.verLibro" 2) bubbles=true}}{{paper-icon "menu_book" class="menu-button-icon"}}<span class="menu-button-typography">Libro 2</span>{{/paper-button}}
    <span class="flex"></span>
    {{#if model.titulo}}
      <div class="header-title">{{model.titulo}}</div>
      <span class="flex"></span>
      <span class="flex"></span>
    {{/if}}
    <NightModeToggle/>
  </PaperToolbarTools>
</PaperToolbar>